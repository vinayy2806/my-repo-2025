pipeline{
    agent any

    tools{
        jdk 'java-11'
        maven 'maven'
    }   

    stages{
        stage('code-chechout'){
            steps{
                git branch:'main',url :'https://github.com/ManojKRISHNAPPA/test-1.git'
            }

        }
        stage('compile'){
            steps{
                sh "mvn compile"

            }
        }
        stage('build'){
            steps{
                sh "mvn clean install"
            }
       }
             
    }
}